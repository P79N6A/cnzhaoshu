import axios from '@/libs/api.request'
import {getAxiosData} from '@/libs/util'
//请求商品合并接口
export const geTmangement = (res) => {
    let data = getAxiosData(res);
    return axios.request({
        url: 'store_management/store_management.php',
        data: data,
        method: 'post'
    })
}
