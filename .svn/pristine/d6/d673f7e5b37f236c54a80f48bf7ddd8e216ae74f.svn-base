                changgeStatus(this,btnname,tender_status)
                let allnum = parseInt(addresnum.text().slice(2))
                if(btnname == "采用"){
                    allnum += tree_num
                }else{
                    allnum -= tree_num
                }
                if(allnum < 0){
                    allnum = 0
                }
                addresnum.text("已采"+allnum+"株");
