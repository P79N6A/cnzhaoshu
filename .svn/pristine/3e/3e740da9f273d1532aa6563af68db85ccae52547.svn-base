st1: [],\r\n      list2: [],\r\n      dropConClass: {\r\n        left: ['drop-box', 'left-drop-box'],\r\n        right: ['drop-box', 'right-drop-box']\r\n      },\r\n      handleList: []\r\n    }\r\n  },\r\n  methods: {\r\n    handleChange ({ src, target, oldIndex, newIndex }) {\r\n      this.handleList.push(`${src} => ${target}, ${oldIndex} => ${newIndex}`)\r\n    }\r\n  },\r\n  mounted () {\r\n    getDragList().then(res => {\r\n      this.list1 = res.data\r\n      this.list2 = [res.data[0]]\r\n    })\r\n  }\r\n}\r\n</script>\r\n<style lang=\"less\">\r\n.drag-box-card{\r\n  display: inline-block;\r\n  width: 600px;\r\n  height: 560px;\r\n  .drag-item{\r\n    margin: 10px;\r\n  }\r\n  h3{\r\n    padding: 10px 15px;\r\n  }\r\n  .drop-box{\r\n    border: 1px solid #eeeeee;\r\n    height: 455px;\r\n    border-radius: 5px;\r\n  }\r\n  .left-drop-box{\r\n    margin-right: 10px;\r\n  }\r\n  .right-drop-box{\r\n    //\r\n  }\r\n}\r\n.handle-log-box{\r\n  display: inline-block;\r\n  margin-left: 20px;\r\n  border: 1px solid #eeeeee;\r\n  vertical-align: top;\r\n  width: 200px;\r\n  height: 500px;\r\n  h3{\r\n    padding: 10px 14px;\r\n  }\r\n  .handle-inner-box{\r\n    height: ~\"calc(100% - 44px)\";\r\n    overflow: auto;\r\n    p{\r\n      padding: 14px 0;\r\n      margin: 0 14px;\r\n      border-bottom: 1px dashed #eeeeee;\r\n    }\r\n  }\r\n}\r\n.res-show-box{\r\n  display: inline-block;\r\n  margin-left: 20px;\r\n  border: 1px solid #eeeeee;\r\n  vertical-align: top;\r\n  width: 350px;\r\n  height: 570px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src\\view\\components\\drag-list"}]}