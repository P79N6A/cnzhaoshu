.head_warp{
    position: fixed;
    left: 0;
    right: 0px;
    background: #FFFFFF;
    width: 100%;
    margin-top: -5px;
}
.bid-list{
    padding-top: 100px;
}
.left-height-middle{
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
}
.right-height-middle{
    position: absolute;
    top: 50%;
    right: 0;
    transform: translateY(-50%);
}
.usual-font{
    height: 30px;
    line-height: 30px;
    text-align: center;
}
.bottom-line{
    border-bottom: 1px solid #c7c7c7;
    padding-bottom: 5px;
}
.weui_search_bar:after{
    display: none;
}
.bid-tittle{
    font-size: 1.2em;
    color: #333333;
    font-weight: bold;
}
.bid-one{
    padding: 15px;
    border: 1px solid #999999;
    width: 96%;
    margin: 0 auto;
    margin-top: 5px;
    box-sizing: border-box;
    border-radius: 5px;
}
.bid-info{
    line-height: 14px;
    padding: 0 10px;
    padding-top: 10px;
}
.bid-info span{
    font-size: .8em;
    color: #333333;
    font-weight: bold;
    padding-right: 5px;
    line-height: 14px;
}
.bid-info span:first-of-type{
    padding-left: 0px;
}
.weui-row{

}
.weui-row-left{
    justify-content: flex-start !important;
}
.content-left{
    padding-left: 10px;
    padding-top: 10px;
    font-size: .9em;
    color: #333333;
    height: 1.2em;
    line-height: 1.2em;
    /*font-weight: bold;*/
}
.content-right{
    padding-right: 10px;
    padding-top: 10px;
    font-size: .9em;
    color: #666666;
    line-height: 1.2em;
    margin-left: 1em;
}
.size-weight{
    font-weight: bold;
}
.bid-info-bottom{
    position: relative;
    padding: 6px 0;
}
.bid-info-bottom:after{
    content: " ";
    position: absolute;
    bottom: 0px;
    left: 0px;
    width: 100%;
    height: 1px;
    color: rgb(217, 217, 217);
    transform-origin: 0px 0px 0px;
    transform: scaleY(0.5);
    border-top: 1px solid rgb(217, 217, 217);
}
.bid-info-bottom:last-of-type:after{
    display: none;
}
.addimg{
    width: 3em;
    height: 3em;
    border-radius: 5px;
    position: relative;
    border: 1px solid rgb(217, 217, 217);
}
.addimg:after{
    content: " ";
    position: absolute;
    top: 50%;
    left: 50%;
    width: 2em;
    height: 1px;
    color: #666666;
    transform-origin: 0px 0px 0px;
    transform: scaleY(0.5) translateX(-50%) ;
    border-top: 1px solid #666666;
}
.bid-info-img{
    width: 2.5rem;
    height: 2.5rem;
    margin-top: .5rem;
    margin-left: 1rem;
}
.bid-info-img img{
    display: block;
    width: 2.5rem;
    height: 2.5rem;
}
.bid-info-imgwarp{
    padding:15px 15px 15px 15px;
    width: 100%;
    box-sizing: border-box;
}
.bid-info-imgwarp > div{
    float: left;
}
.bid-info-imgs{
    float: left;
    width: 100%;
}
.clearfix:after{
    display: block;
    content: "";
    clear: both;
    height: 0;

}
.bid-info-imgs > div{
    float: left;
}
.bid-info-img-one{
    width: 2.5rem;
    height: 2.5rem;
    position: relative;
    margin-right: .5rem;
    margin-top: .5rem;
}
.bid-info-img-one img{
    display: block;
    width: 2.5rem;
    height: 2.5rem;

}
#city-picker{
    outline: none;
    border: none;
    border-radius: 5px;
    text-align: center;
    height: 1rem;
}
.bid-info-img-close{
    position: absolute;
    top: -.4rem;
    right: -.4rem;
    width: 1rem;
    height: 1rem;
    border-radius: .8rem;
    background: #FFFFFF;
    color: #0bb20c;
    line-height: 1rem;
    text-align: center;
    font-size: .8rem;
}
.bid-info-need{
    background: #0bb20c;
    padding: 15px 0;
}
.bid-price{
    text-align: center;
    color: #fff;
}
.bid-num{
    text-align: center;
    color: #fff;
}
.bid-price-input{

}
.bid-price-input input,.bid-num-input input{
    margin: 0 auto;
    width: 6em;
    height: 1.3rem;
    border-radius: 5px;
    margin-left: 1em;
    outline: none;
    border: none;
}
.bid-info-sub{
    display: block;
    outline: none;
    border: none;
    background: #FFFFFF;
    color: #0bb20c;
    margin: 0 auto;
    margin-top: 10px;
    width: 80%;
    height: 2.2em;
    border-radius: 10px;
    font-size: 1.2em;
}
.bid-info-need{
    position: fixed;
    bottom: 0px;
    left: 0px;
    right: 0px;
}
